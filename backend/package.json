{
  "name": "stream-api",
  "version": "1.0.0",
  "description": "backend for MVP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "yarn run build && yarn run watch",
    "build": "yarn run build-ts && yarn run copy-static-assets",
    "serve": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve\" \"yarn run copy-static-assets-watch\"",
    "test": "../backend-unit-testing.sh",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "copy-static-assets": "copyfiles -u 1 'src/**/*.json' 'src/**/*.sql' 'dist/'",
    "copy-static-assets-watch": "copyfiles -w -u 1 'src/**/*.json' 'src/**/*.sql' 'dist/'",
    "debug": "yarn run build && yarn copy-static-assets-watch && yarn run watch-debug",
    "serve-debug": "nodemon --inspect=0.0.0.0:9229 dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\""
  },
